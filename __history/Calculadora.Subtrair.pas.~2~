unit Calculadora.Subtrair;

interface

uses Calculadora.ICalculadora;

type
  TSubtrair = class(TInterfacedObject, iCalculadora)
  private
  public
    function Operacao(Numero1, Numero2: Currency): Currency;
  end;

implementation

{ TSubtrair }

function TSubtrair.Operacao(Numero1, Numero2: Currency): Currency;
begin
  Result := Numero1 - Numero2;
end;

end.
