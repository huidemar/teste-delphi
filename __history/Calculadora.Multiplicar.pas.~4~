unit Calculadora.Multiplicar;

interface

uses Calculadora.ICalculadora;

type
  TMultiplicar = class(TInterfacedObject, iCalculadora)
  private
  public
    constructor Create;
    destructor Destroy; override;
    function Operacao(Numero1, Numero2: Currency): Currency;
  end;

implementation

{ TMultiplicar }

constructor TMultiplicar.Create;
begin

end;

destructor TMultiplicar.Destroy;
begin

  inherited;
end;

function TMultiplicar.Operacao(Numero1, Numero2: Currency): Currency;
begin
  Result := Numero1 * Numero2;
end;

end.
