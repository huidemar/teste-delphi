unit Multiplicar;

interface

uses Calculadora;

type
  TMultiplicar = class(TCalculadora)
  private
  public
    function Operacao(Numero1, Numero2: Currency): Currency; override;
  end;

implementation

{ TMultiplicar }

function TMultiplicar.Operacao(Numero1, Numero2: Currency): Currency;
begin
  Result := Numero1 * Numero2;
end;

end.
