unit Calculadora.Somar;

interface

uses Calculadora.ICalculadora;

type
  TSomar = class(TCalculadora)
  private
  public
    constructor Create;
    destructor Destroy;
    function Operacao(Numero1, Numero2: Currency): Currency; override;
  end;

implementation

{ TSomar }

constructor TSomar.Create;
begin

end;

destructor TSomar.Destroy;
begin

end;

function TSomar.Operacao(Numero1, Numero2: Currency): Currency;
begin
  Result := Numero1 + Numero2;
end;

end.
